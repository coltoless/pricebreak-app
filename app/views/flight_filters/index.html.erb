<script>
function swapCities() {
  const fromInput = document.querySelector('.from-field .search-input');
  const toInput = document.querySelector('.to-field .search-input');
  const temp = fromInput.value;
  fromInput.value = toInput.value;
  toInput.value = temp;
}

// Trip type toggle functionality
document.querySelectorAll('.trip-btn').forEach(btn => {
  btn.addEventListener('click', function() {
    document.querySelectorAll('.trip-btn').forEach(b => b.classList.remove('active'));
    this.classList.add('active');
  });
});
</script>

